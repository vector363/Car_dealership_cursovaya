<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_selected="true">
    <shape android:shape="rectangle">
        <solid android:color="@color/colorSwitch" />
        <corners android:radius="16dp" />
    </shape>
</item>
<item>
    <shape android:shape="rectangle">
        <solid android:color="@color/UnswitchColor" />
        <stroke
            android:width="1dp"
            android:color="@color/colorSwitch" />
        <corners android:radius="16dp" />
    </shape>
</item>
</selector>